	<parameter name="ID_CIDADE" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT a.nome,
	a.matricula,
	a.email,
	c.id AS idcidade,
	c.nome AS nomecidade
FROM aluno a,
	cidade c
WHERE 
	 a.idcidade = c.id  
	 AND 1 = CASE 
	 							WHEN  $P{ID_CIDADE} IS NULL THEN 1
	 							WHEN  $P{ID_CIDADE} = c.id THEN 1
	 							ELSE 0
	 					END	 		
ORDER BY c.nome DESC,
	a.nome DESC]]>
	</queryString>
